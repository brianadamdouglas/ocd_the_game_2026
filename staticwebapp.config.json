{
  "navigationFallback": {
    "rewrite": "/game.html",
    "exclude": ["/images/*", "/css/*", "/dev/*"]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/game.html"
    }
  ],
  "defaultHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' data: blob:;"
  }
}
